TRandom3 *rnd3 = new TRandom3(0);





double gas_pressure1(double s){  //IP and NEG US and DS of the detector. Z300 on e-side of detector

int num1 = floor((5.0 - s)/0.20202020);
int num2 = num1 +1;


return (beam_gas[num1][0] - s)/0.20202020*(beam_gas[num2][1]-beam_gas[num1][1])+beam_gas[num1][1];

}

double gas_pressure6(double s){  //eF IP2, 3, 4 and 5 off

int num1 = floor((5.0 - s)/0.20202020);
int num2 = num1 +1;


return (beam_gas[num1][0] - s)/0.20202020*(beam_gas[num2][2]-beam_gas[num1][2])+beam_gas[num1][2];

}

